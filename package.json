{
    "name": "index-san",
    "version": "0.10.2",
    "description": "A CRUD maker based in filesystem",
    "scripts": {
        "preversion": "npm run lint && npm run test",
        "lint": "npm -w ui run lint",
        "test": "echo \"Error: no test specified\"",
        "dev": "npm -w @index-san/electron run dev",
        "build": "npm -w drive-fsa run build && npm -w ui run build",
        "make:changelog": "node scripts/changelog",
        "changelogen": "changelogen",
        "prepare": "husky install"
    },
    "author": "Henrique Oliveira",
    "license": "MIT",
    "workspaces": {
        "packages": [
            "apps/*",
            "packages/*"
        ]
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@tsconfig/node18": "^18.2.2",
        "@types/node": "^20.17.6",
        "@types/sinon": "^17.0.3",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue-macros/define-emit": "^0.5.1",
        "@vue-macros/define-prop": "^0.6.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-vue": "^9.23.0",
        "fast-glob": "^3.3.2",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "lodash": "^4.17.21",
        "prettier": "3.2.5",
        "sinon": "^17.0.1",
        "typescript": "^5.4.2",
        "vite": "^5.1.6",
        "vitest": "^1.4.0",
        "vue-tsc": "^2.0.6"
    },
    "dependencies": {
        "@vue/tsconfig": "^0.5.1",
        "@vueuse/core": "^11.2.0",
        "changelogen": "^0.5.5",
        "source-map": "^0.7.4",
        "tsup": "^8.0.2",
        "tsx": "^4.7.1",
        "vue": "^3.4.21"
    }
}
