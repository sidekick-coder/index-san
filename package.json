{
    "name": "index-san",
    "version": "0.9.13-alpha",
    "description": "A CRUD maker based in filesystem",
    "scripts": {
        "lint": "npm -ws --if-present run lint",
        "test": "npm -ws --if-present test",
        "dev": "npm -w @index-san/electron run dev",
        "build": "node scripts/build.js",
        "make:version": "node scripts/version.js",
        "make:use-case": "node scripts/new-use-case",
        "make:changelog": "node scripts/changelog",
        "changelogen": "changelogen",
        "prepare": "husky install"
    },
    "author": "Henrique Oliveira",
    "license": "MIT",
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    },
    "devDependencies": {
        "@faker-js/faker": "^8.0.2",
        "@tsconfig/node18": "^18.2.0",
        "@types/node": "^18.16.19",
        "@types/sinon": "^10.0.13",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "@vitejs/plugin-vue": "^4.1.0",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-vue": "^9.11.0",
        "fast-glob": "^3.2.12",
        "jsdom": "^22.1.0",
        "lodash": "^4.17.21",
        "prettier": "3.0.0",
        "sinon": "^15.0.3",
        "typescript": "^4.9.5",
        "vite": "^4.2.1",
        "vitest": "^0.31.1",
        "vue-tsc": "^1.2.0",
        "husky": "^8.0.0"
    },
    "dependencies": {
        "@vue/tsconfig": "^0.4.0",
        "changelogen": "^0.5.3",
        "tsup": "^7.1.0",
        "tsx": "^3.12.7",
        "vue": "^3.3.4"
    }
}
